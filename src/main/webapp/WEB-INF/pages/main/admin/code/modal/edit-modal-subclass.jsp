<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<div class="modal modal-blur fade" id="edit-modal-subclass" tabindex="-1" aria-hidden="true" role="dialog" aria-labelledby="User Edit">
	<div class="modal-dialog modal-lg modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title">Code Edit</h5>
				<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
			</div>
			<div class="modal-body">
				<div class="mb-3">
					<label class="form-label required">Land Use SubClassification Code</label> 
					<input type="text" class="form-control mb-3" placeholder="Land SubCode" disabled="true">
					<div id="editSubclassCodeAlertContainer"></div>
				</div>
				
				<div class="mb-3">
					<label class="form-label required">Land Use Classification Code</label> 
					<input type="text" class="form-control mb-3" placeholder="Land ClassCode" disabled="true">
					<div id="editClassSubCodeAlertContainer"></div>
				</div>
				
				<div class="mb-3">
					<label class="form-label required">Land Use SubClassification in English</label> 
					<input type="text" class="form-control mb-3" placeholder="Land Use SubClassification in English">
					<div id="editSubclassEnAlertContainer"></div>
				</div>
				
				<div class="mb-3">
					<label class="form-label required">Land Use SubClassification in Kirghiz</label> 
					<input type="text" class="form-control mb-3" placeholder="Land Use SubClassification in Kirghiz">
					<div id="editSubclassKyAlertContainer"></div>
				</div>	
				
				<div class="mb-3">
					<label class="form-label required">Land Use SubClassification in Russian</label> 
					<input type="text" class="form-control mb-3" placeholder="Land Use SubClassification in Russian">
					<div id="editSubclassRuAlertContainer"></div>
				</div>
				
				<!-- Field Name 추가 -->
				<div class="mb-3">
					<label class="form-label">Field Name</label> 
					<input type="text" class="form-control" placeholder="Field Name">
				</div>
				
				<div class="mb-3">
					<label class="form-label">Remark</label> 
					<textarea class="form-control" placeholder="Remark"></textarea>
				</div>	
			</div>
			
			<div class="modal-footer justify-content-between">
				<button type="button" class="btn btn-primary" id="saveEditSubClass">SAVE</button>
				<button type="button" class="btn btn-outline-danger" id="deleteEditSubClass">DELETE</button>
                <button type="button" class="btn btn-secondary" id="cancelEditSubClass">CANCEL</button>
			</div>
		</div>
	</div>
</div>
